#include <stdio.h>
#include <math.h>
int main()
{
	float a,b,c,d,e,f,g,h,i,j,k;
	a=pow(1,2);
	b=pow(2,2);
	c=pow(3,2);
	d=pow(4,2);
	e=pow(5,2);
	f=pow(6,2);
	g=pow(7,2);
	h=pow(8,2);
	i=pow(9,2);
	j=pow(10,2);
	k=a+b+c+d+e+f+g+h+i+j;
	printf ("the sum of the square of first 10 natural num:%.2f\n",k);
  return 0;
}
